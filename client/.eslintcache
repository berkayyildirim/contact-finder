  REMOVE_ALERT\n} from \"../types\";\n\nconst AlertState = props => {\n    const initialState = [];\n\n    const [state, dispatch] = useReducer(AlertReducer, initialState);\n\n    //Set Alert\n    const setAlert = (msg, type, timeout = 5000) => {\n        const id = require('uuid').v4\n        dispatch({\n            type: SET_ALERT,\n            payload: {msg, type, id}\n        });\n\n        setTimeout(() => dispatch({type: REMOVE_ALERT, payload: id}), timeout)\n    }\n\n    return (\n        <AlertContext.Provider \n            value={{\n                alerts: state,\n                setAlert\n            }}\n        >\n            {props.children}\n        </AlertContext.Provider>\n    )};\n\nexport default AlertState;","/Users/berkay/Desktop/contact-finder/client/src/context/alert/alertReducer.js",["116"],"import {SET_ALERT, REMOVE_ALERT} from \"../types\";\n\nexport default (state, action) => {\n    switch (action.type) {\n        case SET_ALERT:\n            return [...state, action.payload];\n        case REMOVE_ALERT:\n            return state.filter(alert => alert.id !== action.payload)\n        default: \n            return state;\n    }\n}","/Users/berkay/Desktop/contact-finder/client/src/context/alert/alertContext.js",[],"/Users/berkay/Desktop/contact-finder/client/src/components/layout/Alerts.js",[],"/Users/berkay/Desktop/contact-finder/client/src/utils/setAuthToken.js",[],"/Users/berkay/Desktop/contact-finder/client/src/components/routing/PrivateRoute.js",[],"/Users/berkay/Desktop/contact-finder/client/src/components/layout/Spinner.js",[],{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","severity":1,"message":"122","line":2,"column":8,"nodeType":"123","messageId":"124","endLine":2,"endColumn":11},{"ruleId":"125","severity":1,"message":"126","line":14,"column":1,"nodeType":"127","endLine":79,"endColumn":3},{"ruleId":"121","severity":1,"message":"128","line":3,"column":10,"nodeType":"123","messageId":"124","endLi